 package newstudents;
public class Newstudents {
 String name;
 String city;
 int age;
 public static void main(String[] args) {
 Newstudents s1 =new Newstudents();
 Newstudents s2 =new Newstudents();
 s1.name="Asma";
 s2.name="Amna";
 s1.city="Islamabad";
 s2.city="Muzafrabad";
 s1.age=19;
 s2.age=20;
 System.out.println(s1.name);
 System.out.println(s2.name);
 System.out.println(s1.city);
 System.out.println(s2.city);
 System.out.println(s1.age);
 System.out.println(s2.age);
 }

}
